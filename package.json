{
  "name": "boss-bar-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "html2canvas": "^1.4.1",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open --e2e",
    "cypress:run": "cypress run --e2e"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "include": [
      "/"
    ],
    "inlineCss": true,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "sourceMaps": false,
    "removeBlobs": true,
    "fixWebpackChunks": true
  },
  "devDependencies": {
    "@types/cypress": "^1.1.6",
    "cypress": "^14.5.2",
    "react-snap": "^1.23.0"
  },
  "homepage": "https://bossbargenerator.simonerescio.it"
}
